{% extends 'base.html.twig' %}

{% block stylesheets %}
    <!-- Bootstrap Core CSS -->
    <link href="{{ asset('bundles/app/css/bootstrap.min.css') }}" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="{{ asset('bundles/app/css/metisMenu.min.css') }}" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="{{ asset('bundles/app/css/sb-admin-2.min.css') }}" rel="stylesheet">

    <!-- Morris Charts CSS -->
    <link href="{{ asset('bundles/app/css/morris.css') }}" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="{{ asset('bundles/app/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">

    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- CSS defining icons (required) -->
    <link rel="stylesheet" href="{{ asset('bundles/app/cryptocoins/webfont/cryptocoins.css') }}">

    <!-- CSS defining icon/coin colors (optional) -->
    <!--link rel="stylesheet" href="webfont/cryptocoins-colors.css"-->

    <link rel="stylesheet" href="{{ asset('bundles/app/css/styles.css') }}">
{% endblock %}

{% block body %}
     <div id="wrapper">
         {% include '::navbar.html.twig' %}

        <div id="page-wrapper" style="{% if not app.user %}margin:0px;{% endif %}">
            {% block content %}{% endblock %}
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    <!-- jQuery -->
    <script src="{{ asset('bundles/app/js/jquery.js') }}"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="{{ asset('bundles/app/js/bootstrap.js') }}"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="{{ asset('bundles/app/js/metisMenu.min.js') }}"></script>

    <!-- Morris Charts JavaScript -->
    <script src="{{ asset('bundles/app/js/raphael.min.js') }}"></script>
    <script src="{{ asset('bundles/app/js/morris.min.js') }}"></script>

    <!-- Custom Theme JavaScript -->
    <script src="{{ asset('bundles/app/js/sb-admin-2.min.js') }}"></script>

    <script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>
{% endblock %}
{% extends 'body.html.twig' %}

{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{% block content_title %}{{ title }}{% endblock %}</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>

    <div class="panel panel-default">
        <!-- /.panel-heading -->
        <div class="panel-body">
            {% block panel %}{% endblock %}
        </div>
        <!-- /.panel-body -->
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    {% block content_js %}{% endblock %}
{% endblock %}